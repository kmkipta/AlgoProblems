/**
 * Definition for singly-linked list.
 * function ListNode(val) {
 *     this.val = val;
 *     this.next = null;
 * }
 */
/**
 * @param {ListNode} l1
 * @param {ListNode} l2
 * @return {ListNode}
 */
var mergeTwoLists = function(l1, l2) {
    var l3 = ListNode(0);
    var l1c;
    var l2c;
    
    while(1) {
        if (l1 == null || l2 == null)
            break;
        if (l1 <= l2) {
            l3.next = l1;
            l1 = l1.next;
        }
    }

};
